&HEAD CHID='param1', TITLE='param2' / 

--param3

-- 16 Coarse meshes(for testing) aroud the fuel beds. can be made more fine
&MULT ID='fire_block',DX=1.5,DY=1.5,DZ=1.5,I_LOWER=0,I_UPPER=3,J_LOWER=0,J_UPPER=1,K_LOWER=0,K_UPPER=1 /
&MESH IJK=param7,param7,param7,XB=0,1.5,-1.50,0,0,1.5,MULT_ID='fire_block'/

-- 6 Coarse meshes along the middle, both on top, upwing and downwind of the fuel beds
&MULT ID='cntr_blocks',DX=3,DY=3,DZ=3,I_LOWER=0,I_UPPER=3,I_LOWER_SKIP=1,I_UPPER_SKIP=2,
K_LOWER=0,K_UPPER=1,K_LOWER_SKIP=0,K_UPPER_SKIP=0 /
&MESH IJK=30,30,30,XB=-3, 0,-1.5,1.5,0,3,MULT_ID='cntr_blocks'// (3x3x3)

-- 16 Coarse meshes along bottom and top boundaries, both lower and upper level
&MULT ID='bndry_blocks',DX=3,DY=5.5,DZ=3,I_LOWER=0, I_UPPER=3,
J_LOWER=0,J_UPPER=1,K_LOWER=0,K_UPPER=1 /
&MESH IJK=30,25,30,XB=-3,0,-4.0,-1.5,0,3,MULT_ID='bndry_blocks'/(3x2.5x3)

!SPATIAL AND TEMPORAL DOMAIN
!=========================
&TIME T_END=180 /
&DUMP DT_SLCF=0.5,DT_PART=0.5, DT_ISOF=0.5 /
&MISC HUMIDITY  = param5,TMPA=param4, THICKEN_OBSTRUCTIONS=T/ added thicken obstructions


- Boundary conditions
&VENT XB= -3, -3,-4, 4, 0.0, 6.0, SURF_ID='OPEN' /
&VENT XB=  9,  9,-4, 4, 0.0, 6.0, SURF_ID='OPEN' /
&VENT XB= -3,  9,-4,-4, 0.0, 6.0, SURF_ID='OPEN' /
&VENT XB= -3,  9, 4, 4, 0.0, 6.0, SURF_ID='OPEN' /
&VENT XB= -3,  9,-4, 4, 6.0, 6.0, SURF_ID='OPEN' /


- Specify parame'ters for combustion of fuel gases from pyrolysis of the solid fuel(s)
&REAC FUEL = 'WOOD'
      O          = 2.5
      C          = 3.4
      H          = 6.2
      SOOT_YIELD = 0.02
     HEAT_OF_COMBUSTION = 17700/

&SPEC ID='WATER VAPOR' /

&SURF ID                        = 'wet vegetation'
      MATL_ID                   = 'dry pine'
      MOISTURE_FRACTION         = param6
      SURFACE_VOLUME_RATIO      = 8000.
      LENGTH                    = 0.1
      COLOR                     = 'SILVER'/
      
&MATL ID = 'dry pine'
      DENSITY = 513.6179
      CONDUCTIVITY = 0.1
      SPECIFIC_HEAT= 1.0
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 300.
      NU_MATL = 0.1328
      NU_SPEC = 0.8672
      SPEC_ID = 'WOOD'
      HEAT_OF_REACTION= 1000
      MATL_ID  = 'CHAR' /
 
&MATL ID = 'MOISTURE'
      DENSITY = 1000.
      CONDUCTIVITY = 0.1
      SPECIFIC_HEAT= 4.184
      N_REACTIONS = 1
      REFERENCE_TEMPERATURE = 100.
      NU_SPEC = 1.0
      SPEC_ID = 'WATER VAPOR'
      HEAT_OF_REACTION= 2500. /

&MATL ID = 'CHAR'
      DENSITY  = 200.
      CONDUCTIVITY = 1.0
      SPECIFIC_HEAT = 1.6 /

&PART ID="EXCELSIOR", SAMPLING_FACTOR=1, SURF_ID='wet vegetation', PROP_ID='wood image'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=.TRUE. /


- Vegetative fuel array
&INIT PART_ID="EXCELSIOR", XB=paramf1,paramf2,-0.60,0.60,paramf3,paramf4, 
      N_PARTICLES = paramf5, PACKING_RATIO = 0.0016, MULT_ID = 'FUEL_ARRAY' /
&MULT ID='FUEL_ARRAY',DXB=paramf6,paramf6,0,0,paramf7,paramf7,N_LOWER=0, N_UPPER=paramf8,/


- Fine mesh Platform underneath fuel array and out 40 cm on each side
&SURF ID='platform',COLOR='GRAY'/
&OBST XB=paramf1,paramo2,-1.45,1.45,paramo3,paramo4,SURF_ID='platform', MULT_ID='platform'/ 
&MULT ID='platform',DXB=paramo5,paramo5,0,0,paramo6,paramo6,N_LOWER=0, N_UPPER=paramo7,/


- Ignitor fire
&SURF ID='IGN FIRE',HRRPUA=100.,RAMP_Q='RAMPFIRE',RGB=255,0,0 /
&RAMP ID='RAMPFIRE',T=0, F=0.0 /
&RAMP ID='RAMPFIRE',T=3, F=0.0 /
&RAMP ID='RAMPFIRE',T=4, F=1.0 /
&RAMP ID='RAMPFIRE',T=14,F=1.0 /
&RAMP ID='RAMPFIRE',T=16,F=0.0 /
&VENT XB=paramf1,paramo2,-0.60,0.60,paramo1,paramo1,IOR = 3,SURF_ID='IGN FIRE', MULT_ID = 'ignition'/
&MULT ID='ignition',DXB=paramo5,paramo5,0,0,paramo6,paramo6,N_LOWER=0, N_UPPER=paramo8/


- Outputs for visualization with Smokeview
&SLCF PBY=0,   QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
&SLCF PBZ=0.5, QUANTITY='TEMPERATURE',VECTOR=.TRUE. /
- End of file
&TAIL /
