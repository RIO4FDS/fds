&HEAD CHID='tree_2_m_14_pc', TITLE='2 m Douglas Fir with 14% moisture content' /

&MESH IJK=24,24,24, XB=-1.2,1.2,-1.2,1.2,0.0,2.4, MULT_ID='mesh' /
&MULT ID='mesh', DX=2.4, DY=2.4, DZ=2.4, I_UPPER=1, J_UPPER=0, K_UPPER=3 /

&TIME T_END=30. / 

&REAC FUEL='CELLULOSE', C=3.4, H=6.2, O=2.5, SOOT_YIELD=0.02, HEAT_OF_COMBUSTION=17700. /

&SPEC ID='WATER VAPOR' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /
&VENT PBZ=0.,    SURF_ID='FOIL' /

&SURF ID='FOIL'
      COLOR = 'SILVER'
      MATL_ID='ALUMINUM','CONCRETE'
      THICKNESS(1:2) = 0.0002,0.01 /

&MATL ID='ALUMINUM'
      DENSITY = 2707.
      SPECIFIC_HEAT = 0.896
      CONDUCTIVITY = 204.
      EMISSIVITY = 0.1 /

&MATL ID='CONCRETE'
      DENSITY = 1500.
      SPECIFIC_HEAT = 1.
      CONDUCTIVITY = 0.3  /

&SURF ID                        = 'foliage'
      MATL_ID(1,1:3) = 'comp 1','comp 2','comp 3'
      MATL_MASS_FRACTION(1,1:3) = 0.25,0.50,0.25
      MOISTURE_FRACTION         = 0.14
      SURFACE_VOLUME_RATIO      = 3940.
      LENGTH                    = 0.05
      GEOMETRY                  = 'CYLINDRICAL' /

&SURF ID                        = 'small roundwood'
      MATL_ID(1,1:3) = 'comp 1','comp 2','comp 3'
      MATL_MASS_FRACTION(1,1:3) = 0.25,0.50,0.25
      MOISTURE_FRACTION         = 0.14
      SURFACE_VOLUME_RATIO      = 2667.
      LENGTH                    = 0.10
      GEOMETRY                  = 'CYLINDRICAL' /

&SURF ID                        = 'medium roundwood'
      MATL_ID(1,1:3) = 'comp 1','comp 2','comp 3'
      MATL_MASS_FRACTION(1,1:3) = 0.25,0.50,0.25
      MOISTURE_FRACTION         = 0.14
      SURFACE_VOLUME_RATIO      = 888.
      LENGTH                    = 0.10
      GEOMETRY                  = 'CYLINDRICAL' /

&SURF ID                        = 'large roundwood'
      MATL_ID(1,1:3) = 'comp 1','comp 2','comp 3'
      MATL_MASS_FRACTION(1,1:3) = 0.25,0.50,0.25
      MOISTURE_FRACTION         = 0.14
      SURFACE_VOLUME_RATIO      = 500.
      LENGTH                    = 0.10
      GEOMETRY                  = 'CYLINDRICAL' /

&MATL ID                    = 'MOISTURE'
      DENSITY               = 1000.
      CONDUCTIVITY          = 0.20
      SPECIFIC_HEAT         = 4.22
      REFERENCE_TEMPERATURE = 100.
      HEATING_RATE          = 10.
      PYROLYSIS_RANGE       = 100.
      NU_SPEC               = 1.00
      SPEC_ID               = 'WATER VAPOR'
      HEAT_OF_REACTION      = 2259.   /

&MATL ID                    = 'comp 1'
      DENSITY               = 514.
      CONDUCTIVITY          = 0.20
      SPECIFIC_HEAT_RAMP    = 'c_v'
      A                     = 91.22
      E                     = 44500.
      NU_SPEC               = 0.75
      SPEC_ID               = 'CELLULOSE'
      NU_MATL               = 0.26
      MATL_ID               = 'char'
      HEAT_OF_REACTION      = 416.   /

&MATL ID                    = 'comp 2'
      DENSITY               = 514.
      CONDUCTIVITY          = 0.20
      SPECIFIC_HEAT_RAMP    = 'c_v'
      A                     = 3.66E6
      E                     = 1.05E5
      NU_SPEC               = 0.75
      SPEC_ID               = 'CELLULOSE'
      NU_MATL               = 0.26
      MATL_ID               = 'char'
      HEAT_OF_REACTION      = 416. /

&MATL ID                    = 'comp 3'
      DENSITY               = 514.
      CONDUCTIVITY          = 0.20
      SPECIFIC_HEAT_RAMP    = 'c_v'
      A                     = 14.2
      E                     = 49200.
      NU_SPEC               = 0.75
      SPEC_ID               = 'CELLULOSE'
      NU_MATL               = 0.26
      MATL_ID               = 'char'
      HEAT_OF_REACTION      = 416.  /

&MATL ID                 = 'char'
      DENSITY            = 300.
      CONDUCTIVITY       = 0.052
      SPECIFIC_HEAT_RAMP = 'cp_char'
      NU_O2_CHAR         = 1.65
      N_O2=1.
      MAX_REACTION_RATE  = 100.
      REFERENCE_TEMPERATURE=460
      HEATING_RATE=10
      PYROLYSIS_RANGE=40
      SPEC_ID            = 'PRODUCTS','AIR'
      NU_SPEC            = 8.18,-7.2
      MATL_ID            = 'ash'
      NU_MATL            = 0.02
      HEAT_OF_REACTION   = -12000. /

&RAMP ID='cp_char', T=   27, F=0.715 /
&RAMP ID='cp_char', T= 1724, F=2.04 /

&RAMP ID='c_v', T=  20, F=1.184 /
&RAMP ID='c_v', T= 800, F=4.070 /

&MATL ID            = 'ash'
      DENSITY       = 67.
      CONDUCTIVITY  = 0.1
      SPECIFIC_HEAT = 2.0 /

&PART ID='foliage', DRAG_COEFFICIENT=2.8, SAMPLING_FACTOR=10, SURF_ID='foliage'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS', STATIC=.TRUE., COLOR='FOREST GREEN' /
&INIT PART_ID='foliage', XYZ=0.0,0.0,0.15, RADIUS=0.85, HEIGHT=1.9, SHAPE='CONE', N_PARTICLES_PER_CELL=1, PACKING_RATIO=0.0032 /

&PART ID='small roundwood', DRAG_COEFFICIENT=2.8, SAMPLING_FACTOR=10, SURF_ID='small roundwood'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=.TRUE., COLOR='BROWN' /
&INIT PART_ID='small roundwood', XYZ=0.0,0.0,0.15, RADIUS=0.85, HEIGHT=1.9, SHAPE='CONE', N_PARTICLES_PER_CELL=1, PACKING_RATIO=0.00055 /

&PART ID='medium roundwood', DRAG_COEFFICIENT=2.8, SAMPLING_FACTOR=10, SURF_ID='medium roundwood'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=.TRUE., COLOR='BROWN' /
&INIT PART_ID='medium roundwood', XYZ=0.0,0.0,0.15, RADIUS=0.85, HEIGHT=1.9, SHAPE='CONE', N_PARTICLES_PER_CELL=1, PACKING_RATIO=0.0005 /

&PART ID='large roundwood', DRAG_COEFFICIENT=2.8, SAMPLING_FACTOR=10, SURF_ID='large roundwood'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=.TRUE., COLOR='BROWN' /
&INIT PART_ID='large roundwood', XYZ=0.0,0.0,0.15, RADIUS=0.85, HEIGHT=1.9, SHAPE='CONE', N_PARTICLES_PER_CELL=1, PACKING_RATIO=0.00075 /

&PART ID='ignitor', SAMPLING_FACTOR=1, SURF_ID='burner', STATIC=.TRUE. /
&SURF ID='burner', TMP_FRONT=1500., EMISSIVITY=1., HRRPUA=60., RAMP_T='ignite', RAMP_Q='ignite', RADIUS=0.01, GEOMETRY='SPHERICAL' /
&RAMP ID='ignite', T= 0.0, F=0. /
&RAMP ID='ignite', T= 0.1, F=1. /
&RAMP ID='ignite', T=10.0, F=1. /
&RAMP ID='ignite', T=10.1, F=0. /
&INIT PART_ID='ignitor', XYZ=0.0,0.0,0.16, RADIUS=0.40, SHAPE='RING', N_PARTICLES=400  /

&DUMP DT_HRR=1., DT_DEVC=1. /

&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, QUANTITY='MPUV', PART_ID='foliage', ID='foliage', STATISTICS='VOLUME INTEGRAL'  /
&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, QUANTITY='MPUV', PART_ID='small roundwood', ID='small roundwood', STATISTICS='VOLUME INTEGRAL'  /
&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, QUANTITY='MPUV', PART_ID='medium roundwood', ID='medium roundwood', STATISTICS='VOLUME INTEGRAL'  /
&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, QUANTITY='MPUV', PART_ID='large roundwood', ID='large roundwood', STATISTICS='VOLUME INTEGRAL'  /

&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, OUTPUT=F, QUANTITY='MASS FLUX', PART_ID='foliage', ID='foliage mlr', STATISTICS='SURFACE INTEGRAL'/
&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, OUTPUT=F, QUANTITY='MASS FLUX', PART_ID='small roundwood', ID='small roundwood mlr', STATISTICS='SURFACE INTEGRAL'/
&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, OUTPUT=F, QUANTITY='MASS FLUX', PART_ID='medium roundwood', ID='medium roundwood mlr', STATISTICS='SURFACE INTEGRAL'/
&DEVC XB=-1.0,1.0,-1.0,1.0,0.0,4.0, OUTPUT=F, QUANTITY='MASS FLUX', PART_ID='large roundwood', ID='large roundwood mlr', STATISTICS='SURFACE INTEGRAL'/

&CTRL ID='mlr sum',FUNCTION_TYPE='SUM',INPUT_ID='foliage mlr','small roundwood mlr','medium roundwood mlr','large roundwood mlr'/
&DEVC ID='total mlr',CTRL_ID='mlr sum' QUANTITY='CONTROL VALUE'/

&DEVC XYZ=0.50,0.00,0.2, ORIENTATION= 0.000, 0.000,1, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF1'  /
&DEVC XYZ=1.50,0.00,0.2, ORIENTATION= 0.000, 0.000,1, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF2'  /
&DEVC XYZ=2.50,0.00,0.2, ORIENTATION= 0.000, 0.000,1, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF3'  /
&DEVC XYZ=1.93,0.52,0.2, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF4'  /
&DEVC XYZ=1.93,0.52,1.2, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF6'  /
&DEVC XYZ=1.93,0.52,2.3, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF8'  /
&DEVC XYZ=1.93,0.52,3.4, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF9'  /
&DEVC XYZ=1.93,0.52,4.5, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF10' /
&DEVC XYZ=2.90,0.78,0.2, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF12' /
&DEVC XYZ=2.90,0.78,1.2, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF13' /
&DEVC XYZ=2.90,0.78,2.3, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF14' /
&DEVC XYZ=2.90,0.78,3.4, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF15' /
&DEVC XYZ=2.90,0.78,4.5, ORIENTATION=-0.966,-0.259,0, QUANTITY='GAUGE HEAT FLUX GAS', ID='HF16' /

&SLCF PBY=0., QUANTITY='RADIATION LOSS', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='TEMPERATURE', VECTOR=.TRUE. /

&TAIL /
